journal = {Microvascular research},
keywords = {Age Factors,Aging,Animals,Corrosion Casting,Feasibility Studies,Image Processing, Computer-Assisted,Injections,Male,Models, Anatomic,Models, Cardiovascular,Pulmonary Artery,Pulmonary Artery: anatomy {\&} histology,Pulmonary Artery: growth {\&} development,Pulmonary Artery: radiography,Rats,Rats, Sprague-Dawley,Silicone Elastomers,Silicone Elastomers: chemistry,Time Factors,Viscosity,X-Ray Microtomography},
month = {mar},
