journal = {Bio-medical materials and engineering},
keywords = {Algorithms,Animals,Computer Simulation,Cone-Beam Computed Tomography,Cone-Beam Computed Tomography: methods,Fourier Analysis,Insects,Models, Theoretical,Radiographic Image Interpretation, Computer-Assist,Refractometry,Software,X-Ray Microtomography,X-Ray Microtomography: methods,X-Rays},
month = {jan},
