journal = {International journal of computer assisted radiology and surgery},
keywords = {Anatomic,Computer-Assisted,Computer-Assisted: methods,Humans,Imaging,Interventional,Interventional: instrumentation,Liver Diseases,Liver Diseases: surgery,Liver Diseases: ultrasonography,Models,Phantoms,Punctures,Surgery,Ultrasonography},
