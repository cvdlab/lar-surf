journal = {European radiology},
keywords = {Adult,Aorta, Abdominal,Aorta, Abdominal: radiography,Celiac Artery,Celiac Artery: radiography,Female,Hepatic Artery,Hepatic Artery: radiography,Humans,Imaging, Three-Dimensional,Liver,Liver Transplantation,Liver: blood supply,Liver: radiography,Living Donors,Male,Mathematical Computing,Mesenteric Artery, Superior,Mesenteric Artery, Superior: radiography,Middle Aged,Preoperative Care,Radiographic Image Enhancement,Sensitivity and Specificity,Statistics as Topic,Tomography, Spiral Computed},
