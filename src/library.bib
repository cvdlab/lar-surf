@article{VanderVorst2010,
abstract = {After extended liver resection, a remnant liver that is too small can lead to postresection liver failure. To reduce this risk, preoperative evaluation of the future liver remnant volume (FLRV) is critical. The open-source OsiriX PAC software system can be downloaded for free and used by nonradiologists to calculate liver volume using a stand-alone Apple computer. The purpose of this study was to assess the accuracy of OsiriX CT volumetry for predicting liver resection volume and FLVR in patients undergoing partial hepatectomy.},
