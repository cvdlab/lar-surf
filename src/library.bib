@article{Sharon2000,
abstract = {We introduce a fast, multiscale algorithm for image segmentation.$\backslash$nOur algorithm uses modern numeric techniques to find an approximate$\backslash$nsolution to normalized cut measures in time that is linear in the size$\backslash$nof the image with only a few dozen operations per pixel. In just one$\backslash$npass the algorithm provides a complete hierarchical decomposition of the$\backslash$nimage into segments. The algorithm detects the segments by applying a$\backslash$nprocess of recursive coarsening in which the same minimization problem$\backslash$nis represented with fewer and fewer variables producing an irregular$\backslash$npyramid. During this coarsening process we may compute additional$\backslash$ninternal statistics of the emerging segments and use these statistics to$\backslash$nfacilitate the segmentation process. Once the pyramid is completed it is$\backslash$nscanned from the top down to associate pixels close to the boundaries of$\backslash$nsegments with the appropriate segment. The algorithm is inspired by$\backslash$nalgebraic multigrid (AMG) solvers of minimization problems of heat or$\backslash$nelectric networks. We demonstrate the algorithm by applying it to real$\backslash$nimages},
author = {Sharon, E. and Brandt, a. and Basri, R.},
doi = {10.1109/CVPR.2000.855801},
file = {:C$\backslash$:/Users/Jirik/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Sharon, Brandt, Basri - 2000 - Fast multiscale image segmentation.pdf:pdf},
